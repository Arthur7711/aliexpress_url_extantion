{
  "manifest_version": 3, 
  "name": "Исправитель ссылок на Aliexpress",
  "version": "1.0",
  "description": "Исправляет не правильные ссылки на Aliexpress",
  "icons": {
    "128": "128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://aliexpress.ru/*",
        "https://best.aliexpress.ru/*",
        "https://aliexpress.com/*",
        "https://hz.aliexpress.com/*",
        "https://ru.aliexpress.com/*",
        "https://*.aliexpress.com/*"
      ],
      "js": ["app.js"]
    }
  ]
}
